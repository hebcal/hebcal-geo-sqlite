{
  "name": "@hebcal/geo-sqlite",
  "version": "5.6.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Hebcal ES6 interface to GeoNames and USA ZIP code SQLite databases",
  "module": "./dist/index.js",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "types": "./geo-sqlite.d.ts"
  },
  "bin": {
    "build-geonames-sqlite": "bin/build-geonames-sqlite",
    "download-and-make-dbs": "bin/download-and-make-dbs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-geo-sqlite.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-geo-sqlite/issues"
  },
  "typings": "geo-sqlite.d.ts",
  "engines": {
    "node": ">= 20.0.0"
  },
  "files": [
    "dist",
    "bin",
    "cities-patch.txt",
    "zips-dummy.sql",
    "geo-sqlite.d.ts"
  ],
  "dependencies": {
    "@hebcal/cities": "^6.1.0",
    "@hebcal/core": "^6.0.6",
    "better-sqlite3": "^12.5.0",
    "minimist": "^1.2.8",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "quick-lru": "^7.3.0",
    "transliteration": "^2.3.5"
  },
  "scripts": {
    "build:rollup": "rollup -c",
    "build:version": "node ./version.cjs package.json src/pkgVersion.js",
    "build:json2js": "for f in src/*.json test/*.json; do echo 'export default' > $f.js && cat $f >> $f.js; done",
    "build": "npm run build:version && npm run build:json2js && npm run build:rollup",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "ava": "^6.4.1",
    "eslint": "^9.39.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-n": "^17.23.1",
    "globals": "^16.5.0",
    "jsdoc": "^4.0.5",
    "jsdoc-to-markdown": "^9.1.3",
    "rollup": "^4.53.3"
  }
}
