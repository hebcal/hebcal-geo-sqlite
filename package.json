{
  "name": "@hebcal/geo-sqlite",
  "version": "4.1.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Hebcal ES6 interface to GeoNames and USA ZIP code SQLite databases",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "bin": {
    "build-geonames-sqlite": "bin/build-geonames-sqlite",
    "download-and-make-dbs": "bin/download-and-make-dbs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-geo-sqlite.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-geo-sqlite/issues"
  },
  "typings": "geo-sqlite.d.ts",
  "files": [
    "dist",
    "bin",
    "cities-patch.txt",
    "zips-dummy.sql",
    "geo-sqlite.d.ts"
  ],
  "dependencies": {
    "@hebcal/cities": "^3.1.1",
    "@hebcal/core": "^3.33.3",
    "better-sqlite3": "^7.5.0",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register",
      "regenerator-runtime/runtime"
    ],
    "inherit": true,
    "verbose": true
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "ava": "^4.0.1",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "rollup": "^2.69.1"
  }
}
