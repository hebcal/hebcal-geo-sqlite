{
  "name": "@hebcal/geo-sqlite",
  "version": "3.4.2",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Hebcal ES6 interface to GeoNames and USA ZIP code SQLite databases",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "bin": {
    "build-geonames-sqlite": "bin/build-geonames-sqlite",
    "download-and-make-dbs": "bin/download-and-make-dbs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-geo-sqlite.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-geo-sqlite/issues"
  },
  "typings": "geo-sqlite.d.ts",
  "files": [
    "dist",
    "bin",
    "cities-patch.txt",
    "zips-dummy.sql",
    "geo-sqlite.d.ts"
  ],
  "dependencies": {
    "@hebcal/core": "^3.21.1",
    "better-sqlite3": "^7.4.3",
    "pino": "^7.0.0-rc.1",
    "pino-pretty": "^5.1.2"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.14.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.8",
    "@babel/register": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "ava": "^3.15.0",
    "eslint": "^7.31.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.1",
    "rollup": "^2.53.3"
  }
}
