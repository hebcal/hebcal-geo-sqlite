{
  "name": "@hebcal/geo-sqlite",
  "version": "5.0.5",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Hebcal ES6 interface to GeoNames and USA ZIP code SQLite databases",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./geo-sqlite.d.ts"
  },
  "bin": {
    "build-geonames-sqlite": "bin/build-geonames-sqlite",
    "download-and-make-dbs": "bin/download-and-make-dbs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-geo-sqlite.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-geo-sqlite/issues"
  },
  "typings": "geo-sqlite.d.ts",
  "engines": {
    "node": ">= 20.0.0"
  },
  "files": [
    "dist",
    "bin",
    "cities-patch.txt",
    "zips-dummy.sql",
    "geo-sqlite.d.ts"
  ],
  "dependencies": {
    "@hebcal/cities": "^5.0.1",
    "@hebcal/core": "^5.3.8",
    "better-sqlite3": "^10.0.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.0.0",
    "transliteration": "^2.3.5"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "ava": "^6.1.3",
    "eslint": "^9.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-n": "^17.6.0",
    "globals": "^15.2.0",
    "jsdoc": "^4.0.3",
    "jsdoc-to-markdown": "^8.0.1",
    "rollup": "^4.17.2"
  }
}
